<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Noyacode.DotUtilities" Version="1.1.2-beta" />
    <PackageReference Include="NUnit" Version="3.13.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="$(SolutionDir)input" CopyToOutputDirectory="PreserveNewest" />
    <None Include="$(SolutionDir)input-test" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>

</Project>
